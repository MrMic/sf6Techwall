{% extends "template.html.twig" %}

{% block title %}Liste des todos{% endblock %}

{% block body %}

    {% block PageTitle %}
        Liste des todos :
    {% endblock %}
    <ol class="list-group">
        {% for name, content in app.session.get('todos') %}
        <li class="list-group-item">{{ name }} : {{ content }}</li>
        {% endfor %}
    </ol>

{% endblock %}
